{
  "name": "expo-tracker-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev-all": "concurrently \"npm:codegen-watch\" \"next dev\"",
    "dev": "next dev",
    "dev-tsnode": "nodemon app/api/graphql/server.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen-watch": "graphql-codegen --watch --config codegen.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --skip-seed",
    "db:reset": "prisma migrate reset --skip-seed",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.0.0",
    "@envelop/core": "^5.0.0",
    "@envelop/dataloader": "^6.0.0",
    "@envelop/response-cache": "^6.1.2",
    "@envelop/response-cache-redis": "^4.1.2",
    "@graphql-yoga/redis-event-target": "^3.0.0",
    "@prisma/client": "^5.13.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "dataloader": "^2.2.2",
    "graphql": "^16.8.1",
    "graphql-ws": "^5.16.0",
    "graphql-yoga": "^5.3.0",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "react": "^18",
    "react-dom": "^18",
    "slugify": "^1.6.6",
    "sonner": "^1.4.41",
    "superjson": "^2.2.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.17.0",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@parcel/watcher": "^2.4.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.8",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/ws": "^8.5.10",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "prisma": "^5.13.0",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
